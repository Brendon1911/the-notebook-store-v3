<html>
  <head>
    <title>The Notebook Store</title>
    <link rel="stylesheet" type="text/css" href="client/css/semantic.min.css">
  </head>
  
  <body>
    
    
    <div class="ui large menu">
      <a class="header item">
        The Notebook Store
      </a>
      <a class="item">
        About
      </a>
      <a class="item">
        All Notebooks
      </a>
      <div class="ui dropdown item">
        Categories <i class="dropdown icon"></i>
        <div class="menu">
          <a class="item">14in Notebooks</a>
          <a class="item">15in Notebooks</a>
          <a class="item">17in Notebooks</a>
        </div>
      </div>
      <div class="right menu">
        <div class="item">
          <div class="ui icon input">
            <input type="text" placeholder="Search...">
            <i class="search link icon"></i>
          </div>
        </div>
        <a class="ui item">
          <i class="user plus icon"></i>Register
        </a>
        <a class="ui item">
          <i class="user icon"></i>Login
        </a>
        <a class="ui item">
          <i class="shopping cart icon"></i>Cart
        </a>
      </div>
    </div>
    
    <div class="ui container">
      <h1>
        <%= title %>
      </h1>
      
      <div class="ui link cards">
        <div class="card">
          <div class="image">
            <img src="https://media.loot.co.za/static/gallery/previews/m/m/m/mmmj-5543-g480/preview.mmmj-5543-g480.0.photo.X3RY7HVND6.jpg">
          </div>
          <div class="content">
            <a class="header">Asus VivoBook Max</a>
            <div class="meta">
              <a>15.6"</a>
            </div>
            <div class="description">
              In Stock: 5
            </div>
          </div>
          <div class="extra content">
            <span class="right floated">
              <a class="ui green button">Add to cart</a>
            </span>
            <span>
              <i class="tag icon"></i>
              R 3799
            </span>
          </div>
        </div>
      </div>
    
    <script src="client/js/libs/jquery.min.js"></script>
    <script src="client/js/semantic.min.js"></script>
    <script src="client/js/main.js"></script>
  </body>
</html>